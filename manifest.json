{
    "name": "ReadWrite",
    "version": "0.1",
    "manifest_version": 2,
    "description": "ReadWrite",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "jquery-3.6.0.js",
                "main.js"
            ]
        }
    ],
    "web_accessible_resources":["style.css"],
    "permissions": ["http://*/*"]
}